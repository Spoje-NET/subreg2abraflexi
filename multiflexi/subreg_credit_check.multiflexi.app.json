{
  "$schema": "https://raw.githubusercontent.com/VitexSoftware/php-vitexsoftware-multiflexi-core/refs/heads/main/schema/application.json",
  "schemaVersion": "3.2.1",
  "uuid": "ca0f8b32-836b-421f-80a5-8e382951dcfa",
  "name": {
    "en": "Subreg credit Check",
    "cs": "Kontrola kreditu Subreg"
  },
  "description": {
    "en": "Check Subreg Credit for Customers in AbraFlexi",
    "cs": "Kontrola kreditu Subreg pro zákazníky v AbraFlexi"
  },
  "requirements": [
    "Subreg",
    "AbraFlexi"
  ],
  "tags": [
    "Subreg",
    "Credit",
    "Check",
    "Json"
  ],
  "homepage": "https://github.com/Spoje-NET/subreg2abraflexi/",
  "ociimage": "docker.io/vitexsoftware/subreg2abraflexi",
  "executable": "subreg-check-credit",
  "environment": {
    "ABRAFLEXI_COMPANY": {
      "type": "string",
      "description": {
        "en": "AbraFlexi comapny code",
        "cs": "Kód společnosti v AbraFlexi"
      },
      "defval": "demo_de",
      "required": true,
      "category": "API"
    },
    "ABRAFLEXI_LOGIN": {
      "type": "string",
      "description": {
        "en": "AbraFlexi Login",
        "cs": "AbraFlexi Přihlášení"
      },
      "defval": "winstrom",
      "required": true,
      "category": "API"
    },
    "ABRAFLEXI_PASSWORD": {
      "type": "password",
      "description": {
        "en": "AbraFlexi password",
        "cs": "AbraFlexi heslo"
      },
      "defval": "winstrom",
      "required": true,
      "category": "API"
    },
    "ABRAFLEXI_URL": {
      "type": "string",
      "description": {
        "en": "AbraFlexi Server URI",
        "cs": "AbraFlexi Server URI"
      },
      "defval": "https://demo.flexibee.eu:5434",
      "required": true,
      "category": "API"
    },
    "SUBREG_LOCATION": {
      "type": "string",
      "description": {
        "en": "Subreg API Command Endpoint",
        "cs": "Subreg API Command Endpoint"
      },
      "defval": "https://soap.subreg.cz/cmd.php",
      "required": true,
      "category": "Other"
    },
    "SUBREG_URI": {
      "type": "string",
      "description": {
        "en": "SOAP Endpoint",
        "cs": "SOAP Endpoint"
      },
      "defval": "https://soap.subreg.cz/soap",
      "required": true,
      "category": "Other"
    },
    "SUBREG_LOGIN": {
      "type": "string",
      "description": {
        "en": "Your Subreg username",
        "cs": "Tvé Subreg uživatelské jméno"
      },
      "defval": "",
      "required": true,
      "category": "Other"
    },
    "SUBREG_PASSWORD": {
      "type": "password",
      "description": {
        "en": "Your Subreg password",
        "cs": "Tvé Subreg heslo"
      },
      "defval": "",
      "required": true,
      "category": "Security"
    },
    "ABRAFLEXI_GRANSY_CODE": {
      "type": "password",
      "description": {
        "en": "Gransy Code in AbraFlexi Addressbook",
        "cs": "Kód Gransy v adresáři AbraFlexi"
      },
      "defval": "GRANSY",
      "required": false,
      "category": "API"
    },
    "APP_DEBUG": {
      "type": "bool",
      "description": {
        "en": "Show debug messages",
        "cs": "Zobrazit ladicí zprávy"
      },
      "defval": false,
      "required": false,
      "category": "Behavior"
    },
    "RESULT_FILE": {
      "type": "string",
      "description": {
        "en": "write output json data to",
        "cs": "zapsat výstupní json data do"
      },
      "defval": "subreg_credit_report.json",
      "required": false,
      "category": "Other"
    },
    "ZABBIX_KEY": {
      "type": "string",
      "description": {
        "en": "Default name for Zabbix Item key name",
        "cs": "Výchozí název pro klíč položky Zabbix"
      },
      "defval": "SubregCreditReport",
      "required": false,
      "category": "Other"
    },
    "EASE_LOGGER": {
      "type": "string",
      "description": {
        "en": "write log messages using",
        "cs": "zapsat logovací zprávy pomocí"
      },
      "defval": "console|syslog",
      "required": false,
      "category": "Behavior"
    }
  },
  "exitCodes": {
    "0": {
      "description": {
        "en": "Success",
        "cs": "Úspěch"
      },
      "severity": "success",
      "retry": false
    }
  }
}
