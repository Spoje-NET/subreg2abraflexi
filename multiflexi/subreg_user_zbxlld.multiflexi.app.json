{
  "$schema": "https://raw.githubusercontent.com/VitexSoftware/php-vitexsoftware-multiflexi-core/refs/heads/main/multiflexi.app.schema.json",
  "schemaVersion": "3.2.1",
  "uuid": "804ca7c8-d81e-4d62-9999-1ddcd425b247",
  "name": {
    "en": "Subreg Users LLD for Zabbix",
    "cs": "LLD Subreg uživatelů pro Zabbix"
  },
  "description": {
    "en": "Notify Zabbix about created/removed Subreg users",
    "cs": "Upozornit Zabbix na vytvořené/odstraněné uživatele Subreg"
  },
  "tags": [
    "Subreg",
    "Credit",
    "Check",
    "Json",
    "Zabbix",
    "LLD"
  ],
  "homepage": "https://github.com/Spoje-NET/subreg2abraflexi/",
  "executable": "subregsubaccountslld",
  "environment": {
    "SUBREG_LOCATION": {
      "type": "string",
      "description": {
        "en": "Command Endpoint",
        "cs": "Koncový bod příkazu"
      },
      "defval": "https://soap.subreg.cz/cmd.php",
      "required": true,
      "category": "Other"
    },
    "SUBREG_URI": {
      "type": "string",
      "description": {
        "en": "SOAP Endpoint",
        "cs": "SOAP Endpoint"
      },
      "defval": "https://soap.subreg.cz/soap",
      "required": true,
      "category": "Other"
    },
    "SUBREG_LOGIN": {
      "type": "string",
      "description": {
        "en": "Your Subreg username",
        "cs": "Tvé Subreg uživatelské jméno"
      },
      "defval": "",
      "required": true,
      "category": "Other"
    },
    "SUBREG_PASSWORD": {
      "type": "password",
      "description": {
        "en": "Your Subreg password",
        "cs": "Tvé Subreg heslo"
      },
      "defval": "",
      "required": true,
      "category": "Security"
    },
    "APP_DEBUG": {
      "type": "bool",
      "description": {
        "en": "Show debug messages",
        "cs": "Zobrazit ladicí zprávy"
      },
      "defval": false,
      "required": false,
      "category": "Behavior"
    },
    "RESULT_FILE": {
      "type": "string",
      "description": {
        "en": "write output json data to",
        "cs": "zapsat výstupní json data do"
      },
      "defval": "subreg_credit_report.json",
      "required": false,
      "category": "Other"
    },
    "ZABBIX_KEY": {
      "type": "string",
      "description": {
        "en": "Default name for Zabbix Item key name",
        "cs": "Výchozí název pro klíč položky Zabbix"
      },
      "defval": "SubregCreditReport",
      "required": false,
      "category": "Other"
    },
    "EASE_LOGGER": {
      "type": "string",
      "description": {
        "en": "write log messages using",
        "cs": "zapsat logovací zprávy pomocí"
      },
      "defval": "console|syslog",
      "required": false,
      "category": "Behavior"
    }
  },
  "exitCodes": {
    "0": {
      "description": {
        "en": "Success",
        "cs": "Úspěch"
      },
      "severity": "success",
      "retry": false
    }
  }
}
